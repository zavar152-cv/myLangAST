grammar Example; 
options { 
    language=C; 
    output=AST; 
} 
 
BOOL:         'true' | 'false'; // булевский литерал 
IDENTIFIER:   ('a'..'z' | 'A'..'Z' | '_') ('a'..'z' | 'A'..'Z' | '0'..'9' | '_')*; // идентификатор 
STRING:       '"' ('\\' . | ~('\\'|'"'))* '"'; // строка, окруженная двойными кавычками 
CHAR:         '\'' ~('\'' | '\\') '\''; // одиночный символ в одинарных кавычках 
HEX:          '0' ('x' | 'X') ('0'..'9' | 'a'..'f' | 'A'..'F')+; // шестнадцатеричный литерал 
BITS:         '0' ('b' | 'B') ('0' | '1')+; // битовый литерал 
DEC:          '0'..'9'+; // десятичный литерал 
 
 
listItem 
    : item (',' item)* // список элементов, разделённых запятыми 
    ; 
 
item 
    : IDENTIFIER 
    | STRING 
    | CHAR 
    | HEX 
    | BITS 
    | DEC 
    | BOOL 
    ;
